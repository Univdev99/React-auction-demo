[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/var/www/charibin-tmp/backend
ExecStart=/var/www/charibin-tmp/venv/bin/gunicorn --access-logfile - --error-logfile=/var/log/gunicorn.log --workers 3 --bind unix:/var/www/charibin-tmp/charibin.sock charibin.wsgi:application

[Install]
WantedBy=multi-user.target